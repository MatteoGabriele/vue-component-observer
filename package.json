{
  "name": "vue-component-observer",
  "description": "Plugin for responsive components",
  "version": "2.0.0-beta.0",
  "author": {
    "name": "Matteo Gabriele",
    "email": "m.gabriele.dev@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
