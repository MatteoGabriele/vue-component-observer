  "keywords": [
    "responsive",
    "resize-observer",
    "breakpoints",
    "eqcss",
    "eq",
    "responsive design",
    "vue",
    "vuejs"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@vitejs/plugin-vue": "^2.0.1",
